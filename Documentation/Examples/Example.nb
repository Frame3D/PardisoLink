     RowBox[{"n", ",", "n"}], "\[RightDoubleBracket]"}], "=", "1."}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "The", " ", "following", " ", "guarantees", " ", "that", " ", "L", " ", 
    "is", " ", "CSR", " ", 
    RowBox[{"conform", "."}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"L", "=", 
   RowBox[{"SparseArray", "[", "L", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"b", "=", 
